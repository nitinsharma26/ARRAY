bool subArrayExists(int a[], int n){
    int sum=0;
    
    unordered_map<int,int> mp;
    
    for(int i=0;i<n;i++){
        sum+=a[i];
        if(sum==0) return 1;
        
        else{
            if(mp.find(sum)!=mp.end()){
                return 1;
            }
            else{
                mp[sum]=i;
            }
        }
    }
    return 0;
    
}
